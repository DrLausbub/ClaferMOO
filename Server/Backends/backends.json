{
    "backends": [


        {
            "id": "pythonZ3", 
            "label": "Z3-based IG",
            "tooltip": "A new instance generator based on Z3 solver library",
            "accepted_format": "python",             
            "tool": "sh",
            "tool_args": ["$dirname$/../../../../runClaferZ3", "$filepath$", "-n", "-1", "--showinheritance", "-u", "-g", "5", "--indentation=tab"],            
            "tool_version_args": ["$dirname$/../../../../runClaferZ3", "-v"],
            "scope_options": {
                "set_default_scope" : {"label": "Scope:", "argument": "-g $value$\n", "default_value": 3}
            }            
        },
        {
            "id": "choco_single", 
            "label": "Choco (single objective)",
            "tooltip": "A new Choco solver, for single objective optimization only",
            "accepted_format": "choco",               
            "tool": "java",
            "tool_args": ["-jar", "$dirname$/ChocoSoo/claferchocosoo-0.3.6-jar-with-dependencies.jar", "$filepath$"],
            "tool_version_args": ["-jar", "$dirname$/ChocoSoo/claferchocosoo-0.3.6-jar-with-dependencies.jar", "--version"],
            "scope_options": {
                "set_int_scope" : {"label": "MaxInt:", "argument": "--maxint=$value$", "default_value": 127},
                "set_default_scope" : {"label": "Scope:", "argument": "--scope=$value$\n", "default_value": 25}
            }
        },
        {
            "id": "clafermoo_standard", 
            "label": "ClaferMoo", 
            "tooltip": "Classic Moo based on Alloy solver",
            "accepted_format": "clafer_source",                           
            "tool": "python", 
            "tool_args": ["$dirname$/ClaferMoo/spl_datagenerator/IntegratedFeatureModelOptimizer.py", "$filepath$", "--preservenames", "--donotexecutecompiler"],
            "tool_version_args": ["$dirname$/ClaferMoo/spl_datagenerator/IntegratedFeatureModelOptimizer.py", "--version"],
            "scope_options": {
            }
        }        
    ]   
}